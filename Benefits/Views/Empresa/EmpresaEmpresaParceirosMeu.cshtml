@model IEnumerable<Domain.EmpresaEmpresa>

@{
    ViewData["Title"] = "EmpresaEmpresaParceirosMeu";
}

<h2>EmpresaEmpresaParceirosMeu</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CriadoEm)
            </th>
            <th>
                Empresa
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.CriadoEm)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EmpresaDois.Razao)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.EmpresaEmpresaId">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.EmpresaEmpresaId">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.EmpresaEmpresaId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
