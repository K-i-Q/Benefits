@model Domain.EmpresaEmpresa

@{
    ViewData["Title"] = "EmpresaParceiro";
}
<div class="m-2 container-fluid d-flex justify-content-center p-4 rounded bg-info">
    <h2 class="text-white">Parceria</h2>
</div>
<div class="row d-flex justify-content-center">
    <div class="col-md-4">
        <form asp-action="EmpresaParceiro">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <h2>Confirmar parceria?</h2>
            </div>
            <div class="form-group">
                <input type="submit" value="Confirmar" class="btn btn-block btn-success" />
            </div>
            <div class="form-group">
                <button type="submit" asp-action="Index" asp-controller="Empresa" class="btn btn-block btn-warning">Index</button>
            </div>

        </form>
    </div>
</div>



