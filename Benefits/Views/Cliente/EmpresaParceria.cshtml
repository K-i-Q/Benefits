@model Domain.EmpresaCliente

@{
    ViewData["Title"] = "NIVEL DA PARCERIA";
}
<div class="container-fluid d-flex justify-content-center bg-info p-4 m-4 rounded-pill">
    <h2>Contratação</h2>
</div>
    <hr />
    <div class="row container-fluid d-flex justify-content-center">
        <div class="col-md-4">
            <form asp-action="EmpresaParceria">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="Nivel" class="control-label"></label>
                    <input asp-for="Nivel" class="form-control" />
                    <span asp-validation-for="Nivel" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <input type="submit" value="Contratar" class="btn btn-block btn-warning" />
                    <a asp-action="Index" class="btn btn-block btn-info">Voltar</a>
                </div>
            </form>
        </div>
    </div>

